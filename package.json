{
  "name": "obsidian-magic-mic",
  "version": "1.0.4",
  "description": "Record, transcribe, and summarize audio directly into your Obsidian vault.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write ."
  },
  "keywords": [
    "obsidian",
    "plugin",
    "audio",
    "transcription",
    "speech-to-text",
    "note-taking",
    "productivity"
  ],
  "author": "Drew McDonald",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "dedent": "^1.5.3",
    "dotenv": "^16.4.5",
    "esbuild": "0.17.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "gpt-tokenizer": "^2.2.1",
    "obsidian": "latest",
    "openai": "^4.58.1",
    "prettier": "^3.3.3",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  }
}
